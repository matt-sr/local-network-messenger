    <Text text="Messages:" GridPane.columnIndex="2" GridPane.columnSpan="1" GridPane.rowIndex="1">
      <font>
         <Font size="13.0" />
      </font></Text>
   <Button mnemonicParsing="false" fx:id="send" prefHeight="29.0" prefWidth="77.0" text="Send" GridPane.columnIndex="3" GridPane.rowIndex="3">
      <font>
         <Font size="13.0" />
      </font></Button>
   <TextField prefHeight="25.0" prefWidth="321.0" promptText="Enter Message" GridPane.columnIndex="2" GridPane.rowIndex="3" />
   <ListView prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="2" />
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
